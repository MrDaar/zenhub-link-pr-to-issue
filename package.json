{
  "name": "zenhub-link-pr-to-issue",
  "version": "0.1.0",
  "private": true,
  "description": "Create ZenHub link between pull request and issue based on the branch prefix",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
